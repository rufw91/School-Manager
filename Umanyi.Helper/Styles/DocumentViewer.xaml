                                <Button Content="{StaticResource ZoomInIcon}"
                            ToolTip="Increase the size of the content (Ctrl +)"
                            CommandTarget="{Binding TemplatedParent, RelativeSource={RelativeSource TemplatedParent}}" Command="NavigationCommands.IncreaseZoom"/>
                                <Button Content="{StaticResource ZoomOutIcon}"
                            ToolTip="Increase the size of the content (Ctrl +)"
                            CommandTarget="{Binding TemplatedParent, RelativeSource={RelativeSource TemplatedParent}}" Command="NavigationCommands.DecreaseZoom"/>
                                <Grid Width="15"/>
                                <Button Content="{StaticResource PageHeightIcon}" 
                            ToolTip="Zoom 100% (Ctrl+1)"
                            CommandTarget="{Binding TemplatedParent, RelativeSource={RelativeSource TemplatedParent}}" CommandParameter="100.0" Command="NavigationCommands.Zoom"/>
                                <Button Content="{StaticResource PageWidthIcon}"
                            ToolTip="Page Width (Ctrl+2)"
                            CommandTarget="{Binding TemplatedParent, RelativeSource={RelativeSource TemplatedParent}}" Command="DocumentViewer.FitToWidthCommand"/>
                                <Button Content="{StaticResource FullPageIcon}"
                            ToolTip="Whole Page (Ctrl+3)"
                            CommandTarget="{Binding TemplatedParent, RelativeSource={RelativeSource TemplatedParent}}" CommandParameter="1" Command="DocumentViewer.FitToMaxPagesAcrossCommand"/>
                                <Button Content="{StaticResource TwoPageIcon}"
                            ToolTip="2 Pages (Ctrl+4)"
                            CommandTarget="{Binding TemplatedParent, RelativeSource={RelativeSource TemplatedParent}}" CommandParameter="2" Command="DocumentViewer.FitToMaxPagesAcrossCommand"/>
                            </StackPanel>
                        </Grid>
